{
    "compilerOptions": {
        "outDir": "../OpenRobertaServer/staticResources/js/app/wedoInterpreter",
        "target": "es2017",
        "rootDir": "./ts",
        "typeRoots" : ["./node_modules/@types"],
        "noUnusedLocals": false,
        "noUnusedParameters": false,

        "module": "amd",
        "baseUrl": "./ts",
        "paths": {
            "interpreter.*": ["*"],
            "aiReinforcementLearningModule": ["./aiReinforcementLearningModule/ts/*"],
            "aiNeuralNetworkModule": ["./aiNeuralNetworkModule/source/*"],
            "aiReinforcementLearningModule2": ["./svgPlayground/ts/*"]
        }
    },
    "exclude": [
//        "**/interpreter.robotMbedBehaviour.ts",
        "**/node_modules",
        "**/interpreter.runStackMachineJson.ts"
    ],
    "compileOnSave": true,
    "buildOnSave": true
}