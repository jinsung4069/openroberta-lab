{
    "compilerOptions": {
        "outDir": "../OpenRobertaServer/staticResources/js/app/wedoInterpreter",
        "target": "es2015",
        "rootDir": "./ts",
        "typeRoots" : ["./node_modules/@types"],
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        
        "module": "amd",
        "baseUrl": "./ts",
        "paths": {
            "interpreter.*": ["*"]
        }
    },
    "exclude": [
        "**/node_modules",
        "**/interpreter.runStackMachineJson.ts"
    ],
    "compileOnSave": true,
    "buildOnSave": true
}