{
    "compilerOptions": {
        "outDir": "../OpenRobertaServer/staticResources/js/app/wedoInterpreter",
        "target": "es2017",
        "rootDir": "./ts",
        "typeRoots" : ["./node_modules/@types"],
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        
        "module": "amd",
        "baseUrl": "./ts",
        "paths": {
            "xxxxinterpreter.*": ["*"],
            "aiReinforcementLearningModule": ["./aiReinforcementLearningModule/ts/*"]
        }
    },
    "exclude": [
        "**/node_modules",
        "**/interpreter.runStackMachineJson.ts",
        "**/aiNeuralNetworkModule"
    ],
    "compileOnSave": true,
    "buildOnSave": true
}